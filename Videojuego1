//------------------------------------------------------------------------------------
void mouseClicked(){
  //Cuadro1//
  if((mouseX>=260)&(mouseX<=290)&(mouseY>=230)&(mouseY<=260)){
  rectangulo1(x1,y1,a1,b1);
  m1=1;
  }  //cierre del IF 
  //Cuadro2//
  if((mouseX>=620)&(mouseX<=650)&(mouseY>=230)&(mouseY<=260)){
  rectangulo2(x2,y2,a2,b2);
  m2=1;
  }  //cierre del IF 
  //Cuadro3//
  if((mouseX>=960)&(mouseX<=990)&(mouseY>=230)&(mouseY<=260)){
  rectangulo3(x3,y3,a3,b3);
  m3=1;
  }  //cierre del IF 
   //Cuadro4//
  if((mouseX>=260)&(mouseX<=290)&(mouseY>=600)&(mouseY<=630)){
  rectangulo4(x4,y4,a4,b4);
  m4=1;
  }  //cierre del IF 
  //Cuadro5//
  if((mouseX>=620)&(mouseX<=650)&(mouseY>=600)&(mouseY<=630)){
  rectangulo5(x5,y5,a5,b5);
  m5=1;
  }  //cierre del IF 
  //Cuadro6//
  if((mouseX>=960)&(mouseX<=990)&(mouseY>=600)&(mouseY<=630)){
  rectangulo6(x6,y6,a6,b6);
  m6=1;
  }  //cierre del IF 
  
}  //cierre del MOUSECLIC

void rectangulo1(int x1,int y1,int a1,int b1){
  if(contador<2){
 if(value1==255){
 value1=0;
 ventana=ventana+1;
 rect(x1,y1,a1,b1);
 contador=contador+1;
 print("Se le suma 1");
 }//cierre del IF
 else{
 value1=255;
 contador=contador-1;
 ventana=ventana-1;
 print("Se le resta 1");
   }  //cierre del ELSE
  } //cierre del IF CONTADOR
  else if(contador==2){
  if(value1==0){
    value1=255;
    ventana=ventana-1;
    contador=contador-1;
    }  //cierre IF value1
  }  //cierre del ELSE-IF1
}  //cierre del RECTANGULO1
//RECTANGULO2//
void rectangulo2(int x2,int y2,int a2,int b2){
  if(contador<2){
 if(value2==#EBDEF7){
 value2=0;
 ventana=ventana+1;
 rect(x2,y2,a2,b2);
 contador=contador+1;
 print("Se le suma 1");
 }//cierre del IF
 else{
 value2=(#EBDEF7);
 ventana=ventana-1;
 contador=contador-1;
 print("Se le resta 1");
   }  //cierre del ELSE
  }  //cierre del  IF CONTADOR
  else if(contador==2){
  if(value2==0){
    value2=(#EBDEF7);
    ventana=ventana-1;
    contador=contador-1;
    }  //cierre IF value1
  }  //cierre del ELSE-IF1
}  //cierre del RECTANGULO2
